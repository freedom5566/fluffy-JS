<%- include("layout")%>
    <h1>
        <%= title %>
    </h1>
    <ul>
    <% if(book_list) { %>
        <% for(let book in book_list){ %>
            <li><a href="<%= book_list[book].url %>"> <%= book_list[book].title %></a>(<%= book_list[book].author.name %>)</li>
            
        <% } %>
    <% }else { %>
        <li>There are no books.</li>
    <% } %>
    </ul>
</div>
</div>
</div>
